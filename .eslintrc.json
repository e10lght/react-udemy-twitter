{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard"
    ],
    "overrides": [
        {
            "files": [
                "**/*.js",
                "**/js",
                "**/jsx"
            ],
            "rules": {
                "react/prop-types": "off"
            }
        }
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-uses-react": "off",
        "react/react-in-jsx-scope": "off",
        "space-before-function-paren": "off",
        "indent": "off",
        "quotes": "off",
        "react/prop-types": 0
    }
}